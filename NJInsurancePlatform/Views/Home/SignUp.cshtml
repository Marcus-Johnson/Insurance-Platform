@model NJInsurancePlatform.Models.User

@{
    ViewData["Title"] = "SignUp";
}


<div class="row">
      <div class="col s7 push-s5"><span class="flow-text">
          
  <div class="row">
    <form class="col s12">
      <div class="row">
        <div class="input-field col s12">
          <input placeholder="" id="first_name" type="text" class="validate">
          <label for="first_name">First Name</label>
        </div>
      </div>
      <div class="row">
        <div class="input-field col s12">
          <input placeholder="" id="last_name" type="text" class="validate">
          <label for="last_name">Last Name</label>
        </div>
      </div>
      <div class="row">
        <div class="input-field col s12">
          <input id="email" type="email" class="validate">
          <label for="email">Email</label>
        </div>
      </div>
      <div class="row">
        <div class="input-field col s12">
          <input id="phone_number" type="text" class="validate">
          <label for="phone_number">Phone Number</label>
        </div>
      </div>
      <div class="row">
        <div class="input-field col s12">
          <input id="social_security" type="text" class="validate">
          <label for="social_security">Social Security</label>
        </div>
      </div>
      <div class="row">
        <div class="input-field col s12">
          <input id="license_number" type="text" class="validate">
          <label for="license_number">License Number</label>
        </div>
      </div>
      <div class="input-field col s12">
    <select>
      <option value="" disabled selected>Choose your option</option>
      <option value="1">Option 1</option>
      <option value="2">Option 2</option>
      <option value="3">Option 3</option>
    </select>
    <label>Gender</label>
  </div>
  <div>
      <label>Are you the Primary Account Holder?</label>
      <p>
      <label>
        <input name="group1" type="radio" checked />
        <span>Yes</span>
      </label>
      <label>
        <input name="group1" type="radio" checked />
        <span>No</span>
      </label>
    </p>
    </div>
    </form>
  </div>


      </span></div>
    <div class="col s5 pull-s7"><span class="flow-text">Create an account with us.</span></div>
 </div>