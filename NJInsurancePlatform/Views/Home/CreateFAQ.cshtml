@{
}
@model NJInsurancePlatform.Models.Faq

<form method="post" asp-controller="Home" asp-action="CreateFAQ">
    <div class="collapsible-header">
        @*<i class="material-icons">add</i>*@
        <input type="text" asp-for="Question" placeholder="Question"/>
        <input type="text" asp-for="Answer" placeholder="Answer"/>
        <input readonly type="text" asp-for="FaqMUID" placeholder="@Guid.NewGuid()" />
        <button style="color:white; background-color: #1072ba; margin-left: 2%; box-shadow: 1px 1px #00294d" type="submit" class="policyButton">Create FAQ</button>
    </div>
</form>
